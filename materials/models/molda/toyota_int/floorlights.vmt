"UnlitGeneric"
{
	"$basetexture" "models/molda/toyota_int/bulbs"
	"$surfaceprop" "Metal"

	"$color2" "[0.8 0.8 0.7]"

	"$nodecal" "1"
	"$nocull" "0"

	$color2_on "[1 1 1]"
	$color2_off "[0.1 0.1 0.1]"

	"Proxies"
	{
		TARDIS_Power {
			onVar "$color2_on"
			offVar "$color2_off"
			resultVar "$color2"
		}
		TARDIS_DefaultInt_FloorLightsColor {
			resultVar	$color2_on
		}
	}

}

