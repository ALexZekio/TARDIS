"UnlitGeneric"
{
	"$basetexture" "models/molda/toyota_int/telepathics"

	"$surfaceprop" "Metal"

	"$envmap" "models/molda/toyota_int/environment"
	"$envmaptint" "[0.2 0.45 0.6]"

	"$selfillum" "1"
	"$selfillummask" "models/molda/toyota_int/white"
    $color2 "[0 1.4 1.8]"

	"$nodecal" "1"
	"$nocull" "0"

    $sine 1
    $sine1 1
    $sine2 1
    $sine3 1

    "Proxies"
    {
        Sine
        {
            sineperiod 5
            sinemin -0.2
            sinemax 0.8
            resultVar "$sine"
        }
        Sine
        {
            sineperiod 32
            sinemin -0.8
            sinemax 0.7
            resultVar "$sine1"
        }
        Sine
        {
            sineperiod 16
            timeoffset 16
            sinemin 0.8
            sinemax 1.4
            resultVar "$sine2"
        }
        Sine
        {
            sineperiod 16
            sinemin 1
            sinemax 1.8
            resultVar "$sine3"
        }
        Add
        {
            srcVar1 $sine1
            srcVar2 $sine
            resultVar "$color2[0]"
        }
        Add
        {
            srcVar1 $sine2
            srcVar2 $sine
            resultVar "$color2[1]"
        }
        Add
        {
            srcVar1 $sine3
            srcVar2 $sine
            resultVar "$color2[2]"
        }
    }
}
